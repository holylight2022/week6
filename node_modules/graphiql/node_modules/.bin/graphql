#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//bin/node" ]; then
  "$basedir//bin/node"  "$basedir/../../../graphql-language-service/dist/temp-bin.js" "$@"
  ret=$?
else 
  /bin/node  "$basedir/../../../graphql-language-service/dist/temp-bin.js" "$@"
  ret=$?
fi
exit $ret
